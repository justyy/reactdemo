extends layouts/layout
block content
    div#example
    script(type="text/babel").
        var names = ['A', 'B', 'C']
        ReactDOM.render(
            <div>
            {
                names.map(function(name){
                    return <div>Hello, {name}!</div>
                })
            }
            </div>,
            document.getElementById('example')
        )